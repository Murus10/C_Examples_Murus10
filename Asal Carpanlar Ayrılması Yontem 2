#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <math.h>
#include <time.h>
#include <conio.h>

  int main() {
    int num, i;
    printf("enter the number ");
    scanf("%d", & num);
    for (i = 2; num > 1; i++) {
      while (num % i == 0) {
        printf("%d ", i);
        num = num / i;
      }
    }

    return 0;
  }

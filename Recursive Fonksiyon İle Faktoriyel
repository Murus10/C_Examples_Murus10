#include <conio.h>

 int f(int x);
 int main() {

   int x;
   printf(" Sayi gir ");
   scanf("%d", & x);
   printf("Sonuc; %d", f(x));

 }
 int f(int x) {
   if (x == 0)
     return 1;
   return x * f(x - 1);

 }               

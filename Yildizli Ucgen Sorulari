#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <math.h>
#include <time.h>
#include <conio.h>

              //SOLA YATIK DİK ÜCGEN
int main(){
			char c;
			scanf("%c",&c);
			int i ,j ;
			for(i=1;i<=5;i++){
				 for(j=1;j<=i;j++){
				 	printf("%c",c);
					}
					printf("\n");
			}
			
return 0 ;



